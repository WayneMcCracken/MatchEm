package  {		import flash.events.ProgressEvent;	import flash.events.Event;	import flash.display.MovieClip;	import flash.display.Loader;	import flash.net.URLRequest;	import flash.display.LoaderInfo;		public class LoadProgress extends MovieClip {				private var loadBar: LoadingBar;		private var percLoaded: Number;		public var loader : Loader;				public function LoadProgress() {						loader = new Loader();			drawBar();						loader.load(new URLRequest("MatchEm.swf"));			loader.contentLoaderInfo.addEventListener(Event.COMPLETE, onLoaded);           	loader.contentLoaderInfo.addEventListener(ProgressEvent.PROGRESS, whileProcessing);		}				public function whileProcessing(e: ProgressEvent): void {            percLoaded = Math.round(e.bytesLoaded*100/e.bytesTotal);            loadBar.gotoAndStop(percLoaded);        }       	   public function onLoaded(e: Event): void {            trace("complete");			//addChild(loader); 			loadBar.gotoAndStop(100);			dispatchEvent(new Event("loadComplete"))        }				public function drawBar(): void {			 loadBar = new LoadingBar();			 addChild(loadBar);			 trace("drawing load bar");			 loadBar.stop();		}					}	}